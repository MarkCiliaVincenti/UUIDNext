<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <LangVersion>9</LangVersion>
    <Version>1.0.0</Version>
    <Authors>Matthieu Mourisson</Authors>
    <PackageId>UUIDNext</PackageId>
    <PackageLicenseExpression>WTFPL</PackageLicenseExpression>
    <PackageTags>UUID;GUID;UUID5;UUIDV5;UUID6;UUIDV6;UUID7;UUIDV7</PackageTags>
    <RepositoryUrl>https://github.com/mareek/UUIDNext</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Description>A fast and modern .NET library to generate UUID/GUID that are either name based (versions 3 &amp; 5), sequential (versions 6 &amp; 7) or random (version 4).</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

</Project>
